{
  "name": "tcserver",
  "version": "0.0.1",
  "description": "Tennis Canada Stats Server",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "start:loadTournaments": "ts-node -r tsconfig-paths/register src/loadVRTournaments.ts ",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.1.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/testing": "^8.1.1",
    "@nestjs/typeorm": "8.0.3",
    "@nestjs/websockets": "^8.1.1",
    "@types/superagent": "^4.1.13",
    "@types/xml2js": "^0.4.9",
    "agentkeepalive": "^4.1.4",
    "axios": "^0.23.0",
    "class-transformer": "^0.4.0",
    "csvtojson": "^2.0.10",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "file-saver": "^2.0.5",
    "joi": "^17.4.2",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "passport": "^0.5.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rxjs": "^7.4.0",
    "superagent": "^6.1.0",
    "typeorm": "^0.2.45",
    "typescript": "^4.4.4",
    "xlsx": "^0.17.3",
    "xlsx-populate": "^1.21.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@types/express": "^4.17.13",
    "@types/file-saver": "^2.0.3",
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.2",
    "@types/node": "^16.11.3",
    "@types/supertest": "^2.0.11",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tsconfig-paths": "^3.11.0",
    "tslint": "6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
