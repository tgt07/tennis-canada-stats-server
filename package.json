{
  "name": "tcserver",
  "version": "0.0.1",
  "description": "Tennis Canada Stats Server",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "start:loadTournaments": "ts-node -r tsconfig-paths/register src/loadVRTournaments.ts ",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/testing": "^7.6.18",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.18",
    "@okta/jwt-verifier": "^2.3.0",
    "@types/superagent": "^4.1.12",
    "@types/xml2js": "^0.4.9",
    "agentkeepalive": "^4.1.4",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "csvtojson": "^2.0.10",
    "delay": "^4.4.1",
    "dotenv": "^8.6.0",
    "file-saver": "^2.0.5",
    "joi": "^17.4.1",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rxjs": "^6.6.7",
    "superagent": "^6.1.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.5",
    "xlsx": "^0.16.9",
    "xlsx-populate": "^1.21.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@types/express": "^4.17.13",
    "@types/file-saver": "^2.0.3",
    "@types/jest": "^26.0.24",
    "@types/joi": "^14.3.4",
    "@types/node": "^14.17.5",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "tslint": "6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
